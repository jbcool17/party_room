<!DOCTYPE html>
<html>
<head>
  <title>PartyRoom</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav id='globalNav'>
		<ul>
			<%= link_to root_path do %>
				<li>Home</li>
			<% end %>
			
			<%= link_to cubeworld_path do %>
				<li>Cube World</li>
			<% end %>
		</ul>
	</nav>

	<%= yield %>


	<div id="mp3_player">
		<div id="audio_box"></div>
			<canvas id="analyser_render"></canvas>
		</div>
	</div>
	
	
  <%= javascript_include_tag 'application' %>
</body>
</html>
